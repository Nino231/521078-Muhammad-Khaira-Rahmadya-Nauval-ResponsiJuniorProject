namespace PayAndPerformance.Models
{
    public class StatusKontrak
    {
        private int _id;
        private string _nama = string.Empty;

        public int Id
        {
            get { return _id; }
            set { _id = value; }
        }

        public string Nama
        {
            get { return _nama; }
            set { _nama = value; }
        }
    }
}
